{
  "name": "natninter",
  "version": "0.1.0",
  "description": "Natural neighbor interpolation",
  "repository": "jonathanlurie/natninter",
  "moduleName": "natninter",
  "entry": "src/main.js",
  "main": "dist/natninter.cjs.js",
  "commonjs": "dist/natninter.cjs.js",
  "module": "dist/natninter.module.js",
  "browser": "dist/natninter.umd.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "dev": "serve . & rollup -w -c rollup.config.dev.js",
    "build-min": "npm run build && printf \"// Build date: $(date '+%Y-%m%-dT%H:%M:%S')\n\n\" > dist/natninter.umd.min.js && cat header.txt >> dist/natninter.umd.min.js && google-closure-compiler-js dist/natninter.umd.js >> dist/natninter.umd.min.js",
    "doc": "documentation src/main.js -o ./doc/ -f html",
    "all": "npm run build; npm run build-min; npm run doc"
  },
  "author": "Jonathan Lurie",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "documentation": "^5.4.0",
    "google-closure-compiler-js": "^20170218.0.0",
    "rollup": "^0.56.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "serve": "^6.3.1"
  },
  "dependencies": {
    "area-polygon": "^1.0.1",
    "convex-hull": "^1.0.3",
    "greiner-hormann": "^1.3.4",
    "md5": "^2.2.1",
    "voronoi": "^1.0.0"
  }
}
